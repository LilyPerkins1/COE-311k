   "source": [
    "import random\n",
    "import math\n",
    "\n",
    "def approximatePi(numSamples):\n",
    "    counter = 0\n",
    "    totalCount = 0\n",
    "\n",
    "    for s in range (0, int(numSamples)):\n",
    "        randomX = random.uniform(-1,1)\n",
    "        randomY = random.uniform(-1,1)\n",
    "        \n",
    "        if (math.sqrt(randomX*randomX + randomY*randomY) < 1): ##if radius is less than one, count it\n",
    "            counter = counter + 1\n",
    "            \n",
    "        totalCount = totalCount + 1 ##count total samples\n",
    "        \n",
    "    estPi = 100 * (counter/totalCount)\n",
    "    \n",
    "    return estPi;\n",
    "        \n",
    "        "
   ]
  },
